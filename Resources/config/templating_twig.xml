    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>    
        <parameter key="sorien_datagrid.twig_extention.class">Sorien\DataGridBundle\Twig\DataGridExtension</parameter>
    </parameters>

    <services>
        <service id="sorien_datagrid.twig_extention" class="%sorien_datagrid.twig_extention.class%" public="false">
            <tag name="twig.extension" />
        </service>
    </services>
</container>
